{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 18.12.0 Docs",
  "extends": "@lib/tsconfig/base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "skipDefaultLibCheck": true,
    "exactOptionalPropertyTypes": false,
    "removeComments": true,
    "types": ["node", "vite/client"],
    "paths": {
      "@/*": ["./.vitepress/*"]
    }
  },
  "include": ["src/**/*", ".vitepress/**/*"],
  "exclude": ["**/dist", "**/node_modules", "**/_parts"]
}
