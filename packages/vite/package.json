{
  "name": "@inertiajs-revamped/vite",
  "version": "0.0.1",
  "description": "The Vite plugin for Inertia.js-Revamped",
  "keywords": [
    "inertia",
    "inertiajs",
    "inertiajs-revamped",
    "laravel",
    "preact",
    "react",
    "spa",
    "ssr",
    "vue"
  ],
  "homepage": "https://inertiajs-revamped.com",
  "bugs": {
    "url": "https://github.com/inertiajs-revamped/inertia/issues",
    "email": "github@jrson.me"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inertiajs-revamped/inertia.git",
    "directory": "packages/vite"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": ["dist"],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pkgroll --clean-dist --target=es2020 --minify"
  },
  "devDependencies": {
    "@lib/tsconfig": "workspace:*",
    "@types/node": "^22.6.1"
  },
  "peerDependencies": {
    "laravel-vite-plugin": "1.0.5",
    "vite": "^5.4.7"
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependenciesMeta": {
    "@lib/tsconfig": {
      "optional": true
    }
  }
}
