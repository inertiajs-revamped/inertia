{
  "name": "@inertiajs-revamped/react",
  "version": "0.1.0",
  "description": "The React adapter for Inertia.js-Revamped",
  "keywords": [
    "inertia",
    "inertiajs",
    "inertiajs-revamped",
    "laravel",
    "react",
    "spa",
    "ssr"
  ],
  "homepage": "https://inertiajs-revamped.com",
  "bugs": {
    "url": "https://github.com/inertiajs-revamped/inertia/issues",
    "email": "github@jrson.me"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inertiajs-revamped/inertia.git",
    "directory": "packages/react"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./progress": {
      "require": {
        "types": "./dist/progress.d.cts",
        "default": "./dist/progress.cjs"
      },
      "import": {
        "types": "./dist/progress.d.mts",
        "default": "./dist/progress.mjs"
      }
    },
    "./server": {
      "require": {
        "types": "./dist/server.d.cts",
        "default": "./dist/server.cjs"
      },
      "import": {
        "types": "./dist/server.d.mts",
        "default": "./dist/server.mjs"
      }
    },
    "./vite": {
      "require": {
        "types": "./dist/vite.d.cts",
        "default": "./dist/vite.cjs"
      },
      "import": {
        "types": "./dist/vite.d.mts",
        "default": "./dist/vite.mjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "typesVersions": {
    "*": {
      "progress": ["./dist/progress.d.mts"],
      "server": ["./dist/server.d.mts"],
      "vite": ["./dist/vite.d.mts"]
    }
  },
  "files": ["dist"],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pkgroll --clean-dist --target=es2020 --minify"
  },
  "dependencies": {
    "@inertiajs-revamped/core": "workspace:*",
    "@inertiajs-revamped/vite": "workspace:*",
    "fast-equals": "^5.0.1"
  },
  "devDependencies": {
    "@lib/tsconfig": "workspace:*",
    "@types/node": "^22.13.11",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "peerDependencies": {
    "@types/react": ">=18.3.18",
    "@types/react-dom": ">=18.3.5",
    "react": ">=18.3.1"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependenciesMeta": {
    "@lib/tsconfig": {
      "optional": true
    }
  }
}
